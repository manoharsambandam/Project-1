    {
      "cell_type": "code",
      "metadata": {
        "id": "4Pi-J0I82fMF",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "from keras.layers import Activation, MaxPooling2D\n",
        "\n",
        "model = Sequential() # Create a Sequential model\n",
        "# The following is a convolution layer with 32 channels with 3x3x1 kernel and input data with 28x28x1 matrix\n",
        "# Receptive field is 3x3\n",
        "model.add(Convolution2D(32, 3, 3, activation='relu', input_shape=(28,28,1))) # Output is 32 channels of 26x26 size\n",
        "\n",
        "# added another convolution layer with 64 channels with input kernel of size 3x3x32 ; \n",
        "# Receptive field is 5x5\n",
        "model.add(Convolution2D(64, 3, 3, activation='relu')) # Output is 64 channels of size  24x24\n",
        "\n",
        "# add a convolution layer with 128 channels with input kernel of size 3x3x64  \n",
        "# Receptive field is 7x7\n",
        "model.add(Convolution2D(128, 3, 3, activation='relu')) # output is 128 channels of size 22x22\n",
        "\n",
        "#Do a maxPooling of 2x2 from the input of 128 channels of 22x22 Output will be 128 channels of 11x11 \n",
        "# Receptive field is 9x9\n",
        "model.add(MaxPooling2D(pool_size=(2, 2))) # output is 128 channels of size 11x11 \n",
        "\n",
        "# Assuming the output from the previous layer as 11x11 size of 128 channels\n",
        "# Add a convolution with 256 channel with kernel size 3x3x128\n",
        "# Receptive field is 18x18\n",
        "model.add(Convolution2D(256, 3, 3, activation='relu')) # Output is 256 channels with size 9x9\n",
        "\n",
        "# Add COnvolution layer of   512 output channel with kernel size of 3x3x256\n",
        "# Receptive field is 20x20\n",
        "model.add(Convolution2D(512, 3, 3, activation='relu')) ## Output size will be 7x7x512\n",
        "\n",
        "# Add COnvolution layer of 1024  with kernel size of 3x3x512\n",
        "# Receptive field is 22x22\n",
        "model.add(Convolution2D(1024, 3, 3, activation='relu')) # Output size will be 5x5x1024\n",
        "\n",
        "# Add COnvolution layer of 2048 output channel with kernel size of 3x3x1024\n",
        "# Receptive field is 24x24\n",
        "model.add(Convolution2D(2048, 3, 3, activation='relu')) # output 3x3x2048\n",
        "\n",
        "# Add COnvolution layer of 10 output channel with kernel size of 3x3x2048\n",
        "# Receptive field is 26x26\n",
        "model.add(Convolution2D(10, 3, 3, activation='relu')) #out 1x1 of 10 channels\n",
        "\n",
        "model.add(Flatten())\n",
        "model.add(Activation('softmax'))\n",
        "\n",
        "model.summary()"
      ],
      "execution_count": 0,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "t5Drhhzo2mFD",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "model.compile(loss='categorical_crossentropy',\n",
        "             optimizer='adam',\n",
        "             metrics=['accuracy'])"
      ],
      "execution_count": 0,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "BSQKOMa62ppD",
        "colab_type": "code",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 105
        },
        "outputId": "6193b3ac-23cf-4d2b-ecc8-b23aca223692"
      },
      "source": [
        "model.fit(X_train, Y_train, batch_size=32, nb_epoch=10, verbose=1)"
      ],
      "execution_count": 0,
      "outputs": [
